{
    "name": "bdelespierre/php-phash",
    "description": "pHash implementation in PHP",
    "keywords": [
        "bdelespierre",
        "phash",
        "image hashing algorithm"
    ],
    "homepage": "https://github.com/markoschmidt/php-phash",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Benjamin Delespierre",
            "email": "benjamin.delespierre@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Marko Schmidt",
            "email": "marko.schmidt@mediasignal.fi",
            "role": "Developer"
        }
    ],
    "bin": [
        "bin/phash"
    ],
    "require": {
        "php": "~8.1",
        "intervention/image": "^2.5",
        "symfony/console": "^6.3"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Bdelespierre\\PhpPhash\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    }
}
